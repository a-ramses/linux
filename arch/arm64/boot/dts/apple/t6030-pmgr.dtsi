pmgr0: power-management@290e80000 {
	compatible = "apple,t6030-pmgr", "apple,pmgr", "syscon", "simple-mfd";
	#address-cells = <1>;
	#size-cells = <1>;

	reg = <0x2 0x90e80000 0 0x4000>;
};

